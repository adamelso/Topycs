<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="topycs_web_index_index" pattern="/">
        <default key="_controller">topycs.controller.index:indexAction</default>
    </route>

    <route id="topycs_web_index_latest" pattern="/latest">
        <default key="_controller">topycs.controller.index:latestAction</default>
    </route>

</routes>
